<head>
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
</head>
<body class="site">	
	<div class="container">
		<div class="row navbar navbar-expand-lg navbar-light bg-faded">
			<button class="navbar-toggler ml-auto collapsed" id="btn-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
				<span class="navbar-toggler-icon"></span>
			</button>
		</div>
		<div class="header">
			<div class="header-inner row">
				<div class="col-lg-9">
					<a href="{{link-mainpage}}">
						{{#data-logos}}
							{{^wordmark}}<img src="{{1x}}" width="{{width}}" height="{{height}}">{{/wordmark}}							
						{{/data-logos}}
					</a>
				</div>
				<div class="col-lg-3 row">						
					<div class="col-lg-12 col-6">
						{{^is-registered}}
						<br/>
						{{{html-connexion}}}
						{{/is-registered}}
					</div>
					<div class="col-lg-12 col-6">
						<br/>
						{{{html-search-box}}}
					</div>
				</div>
			</div>
		</div>

		<div class="navbar navbar-expand-lg navbar-transparent navbar-collapse collapse" id="navbarSupportedContent" style="position:relative">
			<ul class="menu navbar-nav me-auto">
				{{>NavBar}}
				{{#is-registered}}
				{{>PersonalUrls}}
				{{>ContentActions}}
				{{>ToolBox}}				
				{{/is-registered}}
			</ul>
		</div>		
		<div class="container">
			<h1 class="firstHeading">{{{html-title}}}</h1>
			{{{html-output}}}
			{{{html-categories}}}
		</div>
		<div class="footer" style="height:60px;">
			<hr>								
			{{#data-footer}}
			<p style="float: left;">
				{{!}}{{#data-icons}}{{>FooterList}}{{/data-icons}}
				{{/data-footer}}
				{{>FooterTexts}}				
			</p>				
			{{>FooterBarLinks}}			
		</div><!--footer -->		
	</div><!--container -->
</body>	
</html>